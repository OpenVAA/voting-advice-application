{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build && ([ -d \"data\" ] && mkdir -p build/data && cp -r data/ build/ || true)",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . ",
    "lint:fix": "prettier --write . && eslint --fix . ",
    "format": "prettier --write .",
    "test": "vitest run tests",
    "test:watch": "vitest tests",
    "coverage": "vitest run tests --coverage",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "algorithm-example": "ts-node-esm --experimentalSpecifierResolution node src/matching/examples/example.ts"
  },
  "lint-staged": {
    "**/*.{svelte,css,html,js,json,md,ts,xml,yaml,yml}": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^7.16",
    "@typescript-eslint/parser": "^7.16",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-svelte": "^2.40.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "svelte-preprocess": "^6.0.1",
    "svelte-visibility-change": "^0.6.0",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^5.7.2",
    "@capacitor/cli": "^5.7.2",
    "@capacitor/core": "^5.7.2",
    "@capacitor/ios": "^5.7.2",
    "@sveltekit-i18n/parser-icu": "^1.0.8",
    "eslint-plugin-import": "^2.29.1",
    "intl-messageformat": "^10.5.14",
    "isomorphic-dompurify": "^2.12.0",
    "sveltekit-i18n": "^2.4.2"
  }
}
