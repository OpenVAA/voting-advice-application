{
  "name": "@openvaa/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build && ([ -d \"data\" ] && mkdir -p build/data && cp -r data/ build/ || true)",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "algorithm-example": "tsx src/matching/examples/example.ts",
    "generate:translation-key-type": "tsx tools/translationKey/generateTranslationKeyType.ts && prettier --write src/lib/types/generated/translationKey.ts",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@openvaa/shared-config": "workspace:^",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-node": "^5.2.11",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^20.17.12",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.23",
    "eslint": "~9.14.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.3",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-eslint-parser": "^0.43.0",
    "svelte-preprocess": "^6.0.3",
    "svelte-visibility-change": "^0.6.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^5.4.20",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.9"
  },
  "engine": {
    "node": "20.18.1",
    "yarn": "4.6",
    "npm": "please-use-yarn"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^5.7.8",
    "@capacitor/cli": "^5.7.8",
    "@capacitor/core": "^5.7.8",
    "@capacitor/ios": "^5.7.8",
    "@openvaa/app-shared": "workspace:^",
    "@openvaa/core": "workspace:^",
    "@openvaa/data": "workspace:^",
    "@openvaa/filters": "workspace:^",
    "@openvaa/matching": "workspace:^",
    "@sveltekit-i18n/parser-icu": "^1.0.8",
    "flat-cache": "^6.1.7",
    "intl-messageformat": "^10.7.11",
    "isomorphic-dompurify": "^2.19.0",
    "jose": "^5.9.6",
    "qs": "^6.13.1",
    "sveltekit-i18n": "^2.4.2"
  },
  "packageManager": "yarn@4.6.0"
}
