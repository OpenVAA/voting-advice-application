{
  "name": "@openvaa/frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build && ([ -d \"data\" ] && mkdir -p build/data && cp -r data/ build/ || true)",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "algorithm-example": "tsx src/matching/examples/example.ts",
    "generate:translation-key-type": "tsx tools/translationKey/generateTranslationKeyType.ts && prettier --write src/lib/types/generated/translationKey.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@openvaa/shared-config": "^1.0.0",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@types/node": "^20.14.5",
    "@typescript-eslint/eslint-plugin": "^7.16",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.12.2",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "svelte-eslint-parser": "^0.41.0",
    "svelte-preprocess": "^6.0.1",
    "svelte-visibility-change": "^0.6.0",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.4"
  },
  "engine": {
    "node": "20.18.1",
    "yarn": "1.22.22",
    "npm": "please-use-yarn"
  },
  "type": "module",
  "dependencies": {
    "@capacitor/android": "^5.7.2",
    "@capacitor/cli": "^5.7.2",
    "@capacitor/core": "^5.7.2",
    "@capacitor/ios": "^5.7.2",
    "@openvaa/app-shared": "^1.0.0",
    "@openvaa/core": "^1.0.0",
    "@openvaa/data": "^1.0.0",
    "@openvaa/filters": "^1.0.0",
    "@openvaa/matching": "^1.0.0",
    "@sveltekit-i18n/parser-icu": "^1.0.8",
    "intl-messageformat": "^10.5.14",
    "isomorphic-dompurify": "^2.12.0",
    "qs": "^6.13.0",
    "sveltekit-i18n": "^2.4.2"
  }
}
