{
  "kind": "collectionType",
  "collectionName": "question_categories",
  "info": {
    "singularName": "question-category",
    "pluralName": "question-categories",
    "displayName": "Question Categories",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "pluginOptions": {},
      "type": "integer",
      "required": true,
      "default": 0
    },
    "elections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::election.election"
    },
    "name": {
      "pluginOptions": {},
      "type": "json"
    },
    "shortName": {
      "pluginOptions": {},
      "type": "json"
    },
    "info": {
      "pluginOptions": {},
      "type": "json"
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question",
      "mappedBy": "category"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "opinion",
        "info"
      ],
      "default": "opinion"
    },
    "color": {
      "type": "string"
    },
    "colorDark": {
      "type": "string"
    },
    "customData": {
      "type": "json"
    },
    "constituencies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::constituency.constituency"
    }
  }
}
