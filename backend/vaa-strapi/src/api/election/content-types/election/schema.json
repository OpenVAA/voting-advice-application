{
  "kind": "collectionType",
  "collectionName": "elections",
  "info": {
    "singularName": "election",
    "pluralName": "elections",
    "displayName": "election",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "electionName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "electionDate": {
      "type": "date",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "electionType": {
      "type": "enumeration",
      "enum": [
        "local",
        "presidential",
        "congress"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "parties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::party.party"
    },
    "electionDescription": {
      "type": "richtext",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "constituencies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::constituency.constituency"
    },
    "candidates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::candidate.candidate",
      "inversedBy": "elections"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    }
  }
}
