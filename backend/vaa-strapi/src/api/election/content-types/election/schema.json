{
  "kind": "collectionType",
  "collectionName": "elections",
  "info": {
    "singularName": "election",
    "pluralName": "elections",
    "displayName": "Election",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "electionName": {
      "type": "string"
    },
    "electionDate": {
      "type": "date"
    },
    "electionType": {
      "type": "enumeration",
      "enum": [
        "local",
        "presidential",
        "congress"
      ]
    },
    "parties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::party.party"
    },
    "electionDescription": {
      "type": "richtext",
      "required": true
    },
    "appTitle": {
      "type": "string"
    },
    "constituencies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::constituency.constituency"
    },
    "topics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::topic.topic"
    }
  }
}
