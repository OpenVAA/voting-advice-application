{
  "kind": "collectionType",
  "collectionName": "candidates",
  "info": {
    "singularName": "candidate",
    "pluralName": "candidates",
    "displayName": "candidate",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "firstName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "lastName": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "motherTongues": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::language.language"
    },
    "otherLanguages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::language.language"
    },
    "politicalExperience": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "otherParties": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::party.party"
    },
    "candidateId": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "candidatePhoto": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "party": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::party.party",
      "inversedBy": "candidates"
    },
    "elections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::election.election",
      "mappedBy": "candidates"
    },
    "answeredQuestions": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "drag-to-reorder.drag-to-reorder-q",
        "likert-scale.likert-five-q",
        "likert-scale.likert-four-q"
      ],
      "required": true
    }
  }
}
