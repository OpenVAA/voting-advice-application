    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@faker-js/faker": "^8.4.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "typescript": "^5.5.2"
