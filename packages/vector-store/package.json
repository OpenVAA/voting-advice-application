{
  "private": true,
  "name": "@openvaa/vector-store",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn tsc --build && yarn tsc-esm-fix && mkdir -p build/core/prompts && cp -R src/core/prompts/* build/core/prompts/",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "type": "module",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    },
    "./types": {
      "types": "./build/types.d.ts",
      "import": "./build/types.js"
    },
    "./utils": {
      "types": "./build/utils.d.ts",
      "import": "./build/utils.js"
    }
  },
  "devDependencies": {
    "@openvaa/shared-config": "workspace:^",
    "openai": "^4.78.1",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@google/genai": "^1.24.0",
    "@openvaa/core": "workspace:^",
    "@openvaa/data": "workspace:^",
    "@openvaa/file-processing": "workspace:^",
    "@openvaa/llm-refactor": "workspace:^",
    "ai": "^5.0.0",
    "chromadb": "^1.10.5",
    "cohere-ai": "^7.19.0"
  }
}
